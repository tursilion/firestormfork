<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="土地出售" name="panel_ls_land">
	<panel name="panel_ls_input">
		<text name="search_text">
			搜尋條件：
		</text>
		<combo_box name="land_category">
			<combo_box.item label="全部" name="All" />
			<combo_box.item label="拍賣" name="Auction" />
			<combo_box.item label="大陸" name="Mainland" />
			<combo_box.item label="領地" name="Estate" />
		</combo_box>
		<check_box label="價格&lt;" name="price_check" />
		<check_box label="面積&gt;" name="area_check" />
		<text name="sort_text">
			按以下方式排序結果：
		</text>
		<check_box label="升序" name="ascending_check" />
		<combo_box name="land_sort_combo">
			<combo_box.item label="名稱" name="Name_item" />
			<combo_box.item label="價格" name="Price_item" />
			<combo_box.item label="平方價格" name="PPM_item" />
			<combo_box.item label="面積" name="Area_item" />
		</combo_box>
		<button label="搜尋" name="land_find" />
	</panel>
	<panel name="panel_ls_scrolllist">
		<scroll_list name="search_results_land">
			<columns label="名稱" relwidth="0.4" name="land_name" />
			<columns label="價格" name="price" />
			<columns label="面積" name="area" />
			<columns label="平方價格" relwidth="0.16" name="ppm" />
			<columns label="類型" name="land_type" />
		</scroll_list>
		<button label="上一頁" name="land_back" />
		<button label="下一頁" name="land_next" />
	</panel>
</panel>