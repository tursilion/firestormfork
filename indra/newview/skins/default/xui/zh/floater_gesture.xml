	<button label="編輯" name="edit_btn" tool_tip="開啟視窗以便編輯所選手勢。" />
	<button label="播放" name="play_btn" tool_tip="在虛擬世界裡運行所選手勢。" />
	<button label="停止" name="stop_btn" />
