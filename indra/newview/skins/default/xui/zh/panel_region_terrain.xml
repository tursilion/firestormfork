<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="地形" name="Terrain">
	<text name="region_text_lbl">
		區域：
	</text>
	<text name="region_text">
		未知
	</text>
	<spinner label="水高度" name="water_height_spin" />
	<spinner label="地形提升限制" name="terrain_raise_spin" />
	<spinner label="地形降低限制" name="terrain_lower_spin" />
	<text name="detail_texture_text">
		地形紋理
	</text>
	<text name="detail_material_text">
		地形材質
	</text>
	<check_box label="PBR金屬粗糙度" name="terrain_material_type" tool_tip="如果選中，使用PBR金屬粗糙度材質來彩現地形。否則，使用紋理。" />
	<text name="height_text_lbl">
		1（低）
	</text>
	<text name="height_text_lbl2">
		2
	</text>
	<text name="height_text_lbl3">
		3
	</text>
	<text name="height_text_lbl4">
		4（高）
	</text>
	<text name="height_text_lbl5">
		紋理海拔範圍
	</text>
	<text name="height_text_lbl5_material">
		材質海拔範圍
	</text>
	<text name="height_text_lbl10">
		這些值代表以上紋理的混合範圍。
	</text>
	<text name="height_text_lbl10_material">
		這些值表示上述材質的混合範圍。
	</text>
	<text name="height_text_lbl11">
		以米為單位，低值是紋理 #1 的最大高度，高值是紋理 #4 的最小高度。
	</text>
	<text name="height_text_lbl11_material">
		以米為單位，低值是材質 #1 的最大高度，高值是材質 #4 的最小高度。
	</text>
	<text name="height_text_lbl6">
		西北
	</text>
	<text name="height_text_lbl7">
		東北
	</text>
	<spinner label="低" name="height_start_spin_1" />
	<spinner label="低" name="height_start_spin_3" />
	<spinner label="高" name="height_range_spin_1" />
	<spinner label="高" name="height_range_spin_3" />
	<text name="height_text_lbl8">
		西南
	</text>
	<text name="height_text_lbl9">
		東南
	</text>
	<spinner label="低" name="height_start_spin_0" />
	<spinner label="低" name="height_start_spin_2" />
	<spinner label="高" name="height_range_spin_0" />
	<spinner label="高" name="height_range_spin_2" />
	<button label="下載 RAW 地形..." name="download_raw_btn" tool_tip="只允許領地所有者而非管理者進行操作" />
	<button label="上傳 RAW 地形檔案..." name="upload_raw_btn" tool_tip="只允許領地所有者而非管理者進行操作" />
	<button label="確定地形" name="bake_terrain_btn" tool_tip="將當前地形設為「升高 / 降低」極限的中心值" />
	<button label="應用" name="apply_btn" />
</panel>